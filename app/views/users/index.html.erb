<div class='container users'>
<h2>Users Of The Application</h2>
<div>
<ul>
Non Admin Users:
<% @users.each do |user| %>
    <% if user.admin %>
    
    <% else%>

    <li><%= user.username %> | <%= link_to 'Delete',user_path(user), method: :delete %></li>
    <% end %>
<% end %>
</ul>
</div>
<hr>
<div>
<ul>
All Users:
<% @users.each do |user| %>
    <% if user.admin != current_user %>
    <li><%= user.username %></li>
    
    <% end %>
<% end %>
</ul>
</div>

<%= link_to 'Back', user_path(current_user) %>
</div>



